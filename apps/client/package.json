    "@types/jsesc": "^3.0.3",
    "@vitejs/plugin-react": "^4",
    "autoprefixer": "^10.4.19",
    "compression": "^1.7.4",
    "eslint": "^9.2.0",
    "express": "^4.18.2",
    "isbot": "^4.3.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.38",
    "serve-static": "^1.15.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.0.13",
    "vite-plugin-babel": "^1.2.0"
  }
